<section id="navbar">
  <div class="ml-auto flex items-center gap-x-6">
    <div class="buttons-group">
      <%= link_to "#", class: 'button' do %>
        <i class="fa-solid fa-shopping-cart w-4 h-4 mr-1"></i>
        Marketplace
      <% end %>
    </div>

    <div class="w-[1px] h-10 bg-neutral-100"></div>
    <%= render 'shared/navbar/select_account' %>
    <div class="w-[1px] h-10 bg-neutral-100"></div>

    <%= button_to destroy_user_session_path, class: 'button', method: :delete do %>
      <i class="fa-solid fa-arrow-right-from-bracket w-3 h-3"></i>
    <% end %>
  </div>
</section>